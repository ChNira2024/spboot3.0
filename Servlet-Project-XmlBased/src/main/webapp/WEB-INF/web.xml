<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Servlet-Project-XmlBased</display-name>
  
  <servlet>
    <servlet-name>servlet-application-xml-based</servlet-name>
    <servlet-class>com.servetInterface.servlet.ServletAppXmlBasedWithDispacther</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet-application-xml-based</servlet-name>
    <url-pattern>/servlet-application-withrd</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>servlet-application-xml-based2</servlet-name>
    <servlet-class>com.servetInterface.servlet.ServletAppXmlBasedWithSendRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet-application-xml-based2</servlet-name>
    <url-pattern>/servlet-application-withsr</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>login-application-xml-based</servlet-name>
    <servlet-class>com.servletInterface.login.LogInAppXmlBasedWithRequestDispacther</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login-application-xml-based</servlet-name>
    <url-pattern>/login-with-requestdispatcher</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>login-application-xml-based2</servlet-name>
    <servlet-class>com.servletInterface.login.LogInAppXmlBasedWithSendRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login-application-xml-based2</servlet-name>
    <url-pattern>/login-with-sendredirect</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>login-application-xml-based-with-cookies</servlet-name>
    <servlet-class>com.httpservletclass.login.LogInAppXmlBasedWithSendRedirectWithCookies</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login-application-xml-based-with-cookies</servlet-name>
    <url-pattern>/login-with-sendredirect-cookies</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>servlet-application-xml-based-with-cookies</servlet-name>
    <servlet-class>com.httpservletclass.servlet.ServletAppXmlBasedWithSendRedirectWithCookis</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>servlet-application-xml-based-with-cookies</servlet-name>
    <url-pattern>/servlet-application-withsr-cookie</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
</web-app>